<Window x:Class="Interfejs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interfejs"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="300" MinWidth="600">
    <Grid>
        <Grid Height="35" VerticalAlignment="Top" HorizontalAlignment="Right" >
            <ComboBox MinWidth="100" HorizontalAlignment="Right" Margin="10,0,10,0" Padding="5,5">
                <ComboBoxItem Content="Page 1" Padding="5,5" Selected="p1Selected"/>
                <ComboBoxItem Content="Page 2" Padding="5,5" Selected="p2Selected"/>
                <ComboBoxItem Content="Page 3" Padding="5,5" Selected="p3Selected"/>
            </ComboBox>


        </Grid>
        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden" SizeChanged="Main_SizeChanged"/>



    </Grid>
</Window>
